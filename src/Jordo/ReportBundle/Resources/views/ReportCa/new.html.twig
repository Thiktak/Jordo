{% extends "ThiktakCoreBundle::layout.html.twig" %}

{% block menu %}

{% endblock %}

{% block body %}
<section>
  <header>
    <hgroup>
      <h1>Raports</h1>
      <h2>Nouveau raport</h2>
    </hgroup>
    <aside>
      <ul>
        <li><a href="{{ path('project_new') }}"><i class="icon-plus"></i></a></li>
      </ul>
    </aside>
  </header>

  <article class="wbox">
    <form action="{{ path('report_ca_create') }}" method="post" {{ form_enctype(form) }}>
