{% extends "ThiktakCoreBundle::layout.html.twig" %}

{% block menu %}
  {% render "JordoUserBundle:User:menu" %}
{% endblock %}

{% block body %}
<section>
  <header>
    <hgroup>
      <h1><a href="{{ path('user') }}">Users</a></h1>
      <h2><a href="{{ path('user_subscription') }}">Papiers</a></h2>
    </hgroup>
    <aside>
      <ul>
        <li>
          <a href="{{ path('user_subscription') }}"><i class="icon-list"></i></a>
        </li>
      </ul>
    </aside>
  </header>

  <article class="wbox">
    <form action="{{ path('user_subscription_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <div class="form-actions">
            <button type="submit" class="btn btn-primary">Create</button>
        </div>
    </form>
  </article>
</section>
{% endblock %}
